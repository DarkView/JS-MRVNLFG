{
  "name": "mrvn",
  "version": "1.0.0",
  "description": "NodeJS version of my Java Discord bot MRVNLFG",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "npx cross-env NODE_ENV=production node dist/index.js",
    "watch": "tsc-watch --onSuccess \"node ./dist/index.js\"",
    "lint": "eslint \"src/**/*.ts\"",
    "lint-fix": "eslint --fix \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "codestyle-check": "prettier --check \"src/**/*.ts\"",
    "build": "npm run format && npm run lint-fix && npx rimraf dist && tsc",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migrate-prod": "npm run typeorm -- migration:run",
    "migrate-dev": "npm run build && npm run typeorm -- migration:run",
    "migrate-rollback-prod": "npm run typeorm -- migration:revert",
    "migrate-rollback-dev": "npm run build && npm run typeorm -- migration:revert"
  },
  "dependencies": {
    "@types/js-yaml": "^3.12.2",
    "@types/node": "^13.13.2",
    "ajv": "^6.12.2",
    "bufferutil": "^4.0.1",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eris": "^0.13.3",
    "fp-ts": "^2.7.0",
    "https": "^1.0.0",
    "humanize-duration": "^3.22.0",
    "io-ts": "^2.2.7",
    "js-yaml": "^3.13.1",
    "knub": "^30.0.0-beta.23",
    "moment-timezone": "^0.5.28",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.0-alpha.24",
    "typescript": "^3.9.6",
    "utf-8-validate": "^5.0.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "sqlite3": "^5.0.0",
    "ts-node": "^8.10.2",
    "tsc-watch": "^4.2.9",
    "tslint": "^6.1.3",
    "typescript-eslint": "0.0.1-alpha.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DarkView/JS-MRVNLFG.git"
  },
  "author": "Nils Bl√∂meke",
  "license": "ISC"
}
