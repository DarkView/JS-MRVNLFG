{
  "name": "mrvn",
  "version": "1.0.0",
  "description": "NodeJS version of my Java Discord bot MRVNLFG",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "npx cross-env NODE_ENV=production node dist/index.js",
    "start-watch": "nodemon",
    "build": "npm run format && npx rimraf dist && tsc",
    "lint": "tslint --project tsconfig.json \"./{src,src/plugins}/*.ts\"",
    "codestyle-check": "prettier --check \"./{src,src/plugins}/*.ts\"",
    "format": "prettier --write \"./{src,src/plugins}/*.ts\""
  },
  "dependencies": {
    "@types/js-yaml": "^3.12.2",
    "@types/node": "^13.13.0",
    "ajv": "^6.12.1",
    "bufferutil": "^4.0.1",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eris": "^0.12.0",
    "https": "^1.0.0",
    "humanize-duration": "^3.22.0",
    "js-yaml": "^3.13.1",
    "knub": "^29.0.2",
    "moment-timezone": "^0.5.28",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "utf-8-validate": "^5.0.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "tslint-config-prettier": "^1.18.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DarkView/JS-MRVNLFG.git"
